{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "paths": {
      "@/relisten/*": [
        "./relisten/*"
      ],
      "@/app/*": [
        "./app/*"
      ],
      "@/modules/*": [
        "./modules/*"
      ],
      "@/assets/*": [
        "./assets/*"
      ]
    }
  },
  "include": [
    "./relisten/**/*.ts",
    "./relisten/**/*.tsx",
    "./relisten/**/*.d.ts",
    "./app/**/*.ts",
    "./app/**/*.tsx",
    "./app/**/*.d.ts",
    "nativewind-env.d.ts"
  ]
}
