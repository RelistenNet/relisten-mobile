{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "experimentalDecorators": true,
    "paths": {
      "@/relisten/*": [
        "./relisten/*"
      ],
      "@/app/*": [
        "./app/*"
      ]
    }
  },
  "include": [
    "./relisten/**/*.ts",
    "./relisten/**/*.tsx",
    "./app/**/*.ts",
    "./app/**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
