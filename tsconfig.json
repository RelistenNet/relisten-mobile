{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "paths": {
      "@/relisten/*": [
        "./relisten/*"
      ],
      "@/app/*": [
        "./app/*"
      ],
      "@/modules/*": [
        "./modules/*"
      ],
      "@/assets/*": [
        "./assets/*"
      ],
      "@g4rb4g3/react-native-carplay": [
        "./node_modules/@g4rb4g3/react-native-carplay/lib/src/index.d.ts"
      ],
      "@g4rb4g3/react-native-carplay/src/*": [
        "./node_modules/@g4rb4g3/react-native-carplay/lib/src/*"
      ],
      "src/*": [
        "./node_modules/@g4rb4g3/react-native-carplay/lib/src/*"
      ],
      "react-native/types": [
        "./node_modules/react-native/types/index.d.ts"
      ]
    }
  },
  "include": [
    "./relisten/**/*.ts",
    "./relisten/**/*.tsx",
    "./relisten/**/*.d.ts",
    "./app/**/*.ts",
    "./app/**/*.tsx",
    "./app/**/*.d.ts",
    "nativewind-env.d.ts"
  ]
}
