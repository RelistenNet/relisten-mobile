{
  "cli": {
    "version": ">= 3.15.1",
    "promptToConfigurePushNotifications": false,
    "appVersionSource": "local"
  },
  "build": {
    "base": {
      "node": "22.14.0"
    },
    "development": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "channel": "production",
      "env": {
        "RELEASE_CHANNEL": "production"
      }
    },
    "testflight": {
      "extends": "production",
      "channel": "testflight",
      "env": {
        "RELEASE_CHANNEL": "testflight"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
